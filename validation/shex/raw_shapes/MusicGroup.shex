<#MusicGroup> @<#PerformingGroup> AND EXTRA a { 
	a [schema:MusicGroup];
	schema:album @<#MusicAlbum> * ;
	schema:albums @<#MusicAlbum> * ;
	schema:genre (@<#Text> OR @<#URL>) * ;
	schema:musicGroupMember @<#Person> * ;
	schema:track (@<#MusicRecording> OR @<#ItemList>) * ;
	schema:tracks @<#MusicRecording> * ;
}