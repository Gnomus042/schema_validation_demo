<#MusicComposition> @<#CreativeWork> AND EXTRA a { 
	a [schema:MusicComposition];
	schema:composer (@<#Organization> OR @<#Person>) * ;
	schema:firstPerformance @<#Event> * ;
	schema:includedComposition @<#MusicComposition> * ;
	schema:iswcCode @<#Text> * ;
	schema:lyricist @<#Person> * ;
	schema:lyrics @<#CreativeWork> * ;
	schema:musicArrangement @<#MusicComposition> * ;
	schema:musicCompositionForm @<#Text> * ;
	schema:musicalKey @<#Text> * ;
	schema:recordedAs @<#MusicRecording> * ;
}