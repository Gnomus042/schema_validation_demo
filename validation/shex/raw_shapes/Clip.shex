<#Clip> @<#CreativeWork> AND EXTRA a { 
	a [schema:RadioClip schema:VideoGameClip schema:MovieClip schema:TVClip schema:Clip];
	schema:actor @<#Person> * ;
	schema:actors @<#Person> * ;
	schema:clipNumber (@<#Text> OR @<#Integer>) * ;
	schema:director @<#Person> * ;
	schema:directors @<#Person> * ;
	schema:musicBy (@<#Person> OR @<#MusicGroup>) * ;
	schema:partOfEpisode @<#Episode> * ;
	schema:partOfSeason @<#CreativeWorkSeason> * ;
	schema:partOfSeries @<#CreativeWorkSeries> * ;
}