<#HowToDirection> @<#CreativeWork> AND EXTRA a { 
	a [schema:HowToDirection];
	schema:afterMedia (@<#MediaObject> OR @<#URL>) * ;
	schema:beforeMedia (@<#URL> OR @<#MediaObject>) * ;
	schema:duringMedia (@<#URL> OR @<#MediaObject>) * ;
	schema:performTime @<#Duration> * ;
	schema:prepTime @<#Duration> * ;
	schema:supply (@<#HowToSupply> OR @<#Text>) * ;
	schema:tool (@<#HowToTool> OR @<#Text>) * ;
	schema:totalTime @<#Duration> * ;
}