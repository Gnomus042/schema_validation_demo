<#Offer> @<#Intangible> AND EXTRA a { 
	a [schema:AggregateOffer schema:Offer];
	schema:acceptedPaymentMethod (@<#LoanOrCredit> OR @<#PaymentMethod>) * ;
	schema:addOn @<#Offer> * ;
	schema:advanceBookingRequirement @<#QuantitativeValue> * ;
	schema:aggregateRating @<#AggregateRating> * ;
	schema:areaServed (@<#AdministrativeArea> OR @<#Text> OR @<#Place> OR @<#GeoShape>) * ;
	schema:availability @<#ItemAvailability> * ;
	schema:availabilityEnds (@<#DateTime> OR @<#Time> OR @<#Date>) * ;
	schema:availabilityStarts (@<#Date> OR @<#Time> OR @<#DateTime>) * ;
	schema:availableAtOrFrom @<#Place> * ;
	schema:availableDeliveryMethod @<#DeliveryMethod> * ;
	schema:businessFunction @<#BusinessFunction> * ;
	schema:category (@<#Thing> OR @<#URL> OR @<#PhysicalActivityCategory> OR @<#Text>) * ;
	schema:deliveryLeadTime @<#QuantitativeValue> * ;
	schema:eligibleCustomerType @<#BusinessEntityType> * ;
	schema:eligibleDuration @<#QuantitativeValue> * ;
	schema:eligibleQuantity @<#QuantitativeValue> * ;
	schema:eligibleRegion (@<#Text> OR @<#Place> OR @<#GeoShape>) * ;
	schema:eligibleTransactionVolume @<#PriceSpecification> * ;
	schema:gtin12 @<#Text> * ;
	schema:gtin13 @<#Text> * ;
	schema:gtin14 @<#Text> * ;
	schema:gtin8 @<#Text> * ;
	schema:includesObject @<#TypeAndQuantityNode> * ;
	schema:inventoryLevel @<#QuantitativeValue> * ;
	schema:itemCondition @<#OfferItemCondition> * ;
	schema:itemOffered (@<#AggregateOffer> OR @<#Product> OR @<#CreativeWork> OR @<#Service> OR @<#MenuItem> OR @<#Trip> OR @<#Event>) * ;
	schema:mpn @<#Text> * ;
	schema:offeredBy (@<#Organization> OR @<#Person>) * ;
	schema:price (@<#Text> OR @<#Number>) * ;
	schema:priceCurrency @<#Text> * ;
	schema:priceSpecification @<#PriceSpecification> * ;
	schema:priceValidUntil @<#Date> * ;
	schema:review @<#Review> * ;
	schema:reviews @<#Review> * ;
	schema:seller (@<#Organization> OR @<#Person>) * ;
	schema:serialNumber @<#Text> * ;
	schema:sku @<#Text> * ;
	schema:validFrom (@<#Date> OR @<#DateTime>) * ;
	schema:validThrough (@<#Date> OR @<#DateTime>) * ;
	schema:warranty @<#WarrantyPromise> * ;
}