<#PriceSpecification> @<#StructuredValue> AND EXTRA a { 
	a [schema:PaymentChargeSpecification schema:DeliveryChargeSpecification schema:CompoundPriceSpecification schema:UnitPriceSpecification schema:PriceSpecification];
	schema:eligibleQuantity @<#QuantitativeValue> * ;
	schema:eligibleTransactionVolume @<#PriceSpecification> * ;
	schema:maxPrice @<#Number> * ;
	schema:minPrice @<#Number> * ;
	schema:price (@<#Text> OR @<#Number>) * ;
	schema:priceCurrency @<#Text> * ;
	schema:validFrom (@<#Date> OR @<#DateTime>) * ;
	schema:validThrough (@<#Date> OR @<#DateTime>) * ;
	schema:valueAddedTaxIncluded @<#Boolean> * ;
}