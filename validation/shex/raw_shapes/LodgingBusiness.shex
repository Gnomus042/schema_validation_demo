<#LodgingBusiness> @<#LocalBusiness> AND EXTRA a { 
	a [schema:Hostel schema:Motel schema:Resort schema:BedAndBreakfast schema:Hotel schema:Campground schema:LodgingBusiness];
	schema:amenityFeature @<#LocationFeatureSpecification> * ;
	schema:audience @<#Audience> * ;
	schema:availableLanguage (@<#Text> OR @<#Language>) * ;
	schema:checkinTime (@<#DateTime> OR @<#Time>) * ;
	schema:checkoutTime (@<#DateTime> OR @<#Time>) * ;
	schema:numberOfRooms (@<#QuantitativeValue> OR @<#Number>) * ;
	schema:petsAllowed (@<#Text> OR @<#Boolean>) * ;
	schema:starRating @<#Rating> * ;
}