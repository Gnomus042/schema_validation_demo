<#CommunicateAction> @<#InteractAction> AND EXTRA a { 
	a [schema:CheckInAction schema:InformAction schema:RsvpAction schema:ShareAction schema:ReplyAction schema:InviteAction schema:CheckOutAction schema:CommentAction schema:AskAction schema:ConfirmAction schema:CommunicateAction];
	schema:about @<#Thing> * ;
	schema:inLanguage (@<#Text> OR @<#Language>) * ;
	schema:language @<#Language> * ;
	schema:recipient (@<#Organization> OR @<#Audience> OR @<#ContactPoint> OR @<#Person>) * ;
}