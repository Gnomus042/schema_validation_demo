<#Occupation> @<#Intangible> AND EXTRA a { 
	a [schema:Occupation];
	schema:estimatedSalary (@<#Number> OR @<#MonetaryAmountDistribution> OR @<#MonetaryAmount>) * ;
	schema:experienceRequirements @<#Text> * ;
	schema:occupationLocation @<#AdministrativeArea> * ;
	schema:responsibilities @<#Text> * ;
	schema:skills (@<#Text> OR @<#DefinedTerm>) * ;
}