<#Rating> @<#Intangible> AND EXTRA a { 
	a [schema:EndorsementRating schema:AggregateRating schema:EmployerAggregateRating schema:Rating];
	schema:author (@<#Person> OR @<#Organization>) * ;
	schema:bestRating (@<#Number> OR @<#Text>) * ;
	schema:ratingValue (@<#Number> OR @<#Text>) * ;
	schema:reviewAspect @<#Text> * ;
	schema:worstRating (@<#Text> OR @<#Number>) * ;
}