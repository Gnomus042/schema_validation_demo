<#Recipe> @<#HowTo> AND EXTRA a {
	a [schema:Recipe];
	schema:cookTime  @<#Duration> ?;
	schema:cookingMethod  @<#Text> ?;
	schema:ingredients  @<#Text> *;
	schema:nutrition  @<#NutritionInformation> ?;
	schema:recipeCategory  @<#Text> ?;
	schema:recipeCuisine  @<#Text> ?;
	schema:recipeIngredient  @<#Text> ?;
	schema:recipeInstructions ( @<#CreativeWork> OR  @<#ItemList> OR  @<#Text>) ?;
	schema:recipeYield (@<#QuantitativeValue> OR @<#Text>) ?;
	schema:suitableForDiet (@<#Text> AND /http:\/\/schema.org\/(DiabeticDiet|GlutenFreeDiet|HalalDiet|HinduDiet|KosherDiet|LowCalorieDiet|LowFatDiet|LowLactoseDiet|LowSaltDiet|VeganDiet|VegetarianDiet)/) OR
	    { a [schema:DiabeticDiet schema:GlutenFreeDiet schema:HalalDiet schema:HinduDiet schema:KosherDiet schema:LowCalorieDiet schema:LowFatDiet schema:LowLactoseDiet schema:LowSaltDiet schema:VeganDiet schema:VegetarianDiet] } ?;
}