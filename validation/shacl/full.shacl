	].


:Integer a sh:NodeShape;
    sh:or (
        [
            sh:node :Text;
            sh:pattern "^([1-9][0-9]*|0)$"
        ]
        [ sh:datatype xsd:integer ]
    ).



:Number a sh:NodeShape;
    sh:or (
        [sh:node :Integer]
        [sh:datatype xsd:decimal]
    ).


