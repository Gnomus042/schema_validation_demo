	sh:property [
		sh:path schema:nutrition;
		sh:node :NutritionInformation;
	];
	sh:property [
		sh:path schema:recipeCategory;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:recipeCuisine;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:recipeIngredient;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:recipeInstructions;
		sh:or (
			[sh:node :CreativeWork]
			[sh:class schema:ItemList]
			[sh:node :Text]
		);
	];
	sh:property [
		sh:path schema:recipeYield;
		sh:or (
			[sh:class schema:QuantitativeValue]
			[sh:node :Text]
		);
	];
	sh:property [
		sh:path schema:suitableForDiet;
		sh:node :RestrictedDiet;
	].


:Text a sh:NodeShape;
    sh:or (
        [sh:datatype xsd:string]
        [sh:nodeKind sh:Literal]
    ).


:Thing a sh:NodeShape;
	sh:targetClass schema:Thing;
	sh:class schema:Thing;
	sh:property [
		sh:path schema:additionalType;
		sh:node :URL;
	];
	sh:property [
		sh:path schema:alternateName;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:description;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:disambiguatingDescription;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:identifier;
		sh:or (
			[sh:node :Text]
			[sh:class schema:PropertyValue]
			[sh:node :URL]
		);
	];
	sh:property [
		sh:path schema:image;
		sh:or (
			[sh:node :URL]
			[sh:class schema:ImageObject]
		);
	];
	sh:property [
		sh:path schema:mainEntityOfPage;
		sh:or (
			[sh:node :CreativeWork]
			[sh:node :URL]
		);
	];
	sh:property [
		sh:path schema:name;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:potentialAction;
		sh:node :Action;
	];
	sh:property [
		sh:path schema:sameAs;
		sh:node :URL;
	];
	sh:property [
		sh:path schema:subjectOf;
		sh:or (
			[sh:class schema:Event]
			[sh:node :CreativeWork]
		);
	];
	sh:property [
		sh:path schema:url;
		sh:node :URL;
	].



:URL a sh:NodeShape;
    sh:pattern "^(https?|gopher|ftps?):" .