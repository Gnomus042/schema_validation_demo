:Date a sh:NodeShape;
    sh:node :Text;
    sh:pattern ".*".


:DateTime a sh:NodeShape;
    sh:node :Text;
    sh:pattern ".*".


:Duration a sh:NodeShape;
    sh:node :Text;
    sh:pattern "^(-?)P(?=[0-9]|T[0-9])(?:([0-9]+)Y)?(?:([0-9]+)M)?(?:([0-9]+)([DW]))?(?:T(?:([0-9]+)H)?(?:([0-9]+)M)?(?:([0-9]+(?:\\.[0-9]+)?)S)?)?$".


