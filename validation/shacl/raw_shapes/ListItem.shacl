:ListItem a sh:NodeShape;
	sh:targetClass schema:ListItem;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:item;
		sh:node :Thing;
	];
	sh:property [
		sh:path schema:nextItem;
		sh:node :ListItem;
	];
	sh:property [
		sh:path schema:position;
		sh:or (
			[sh:node :Integer]
			[sh:node :Text]
		);
	];
	sh:property [
		sh:path schema:previousItem;
		sh:node :ListItem;
	].