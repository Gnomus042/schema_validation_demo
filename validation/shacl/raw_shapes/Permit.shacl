:Permit a sh:NodeShape;
	sh:targetClass schema:Permit;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:issuedBy;
		sh:node :Organization;
	];
	sh:property [
		sh:path schema:issuedThrough;
		sh:node :Service;
	];
	sh:property [
		sh:path schema:permitAudience;
		sh:node :Audience;
	];
	sh:property [
		sh:path schema:validFor;
		sh:node :Duration;
	];
	sh:property [
		sh:path schema:validFrom;
		sh:or (
			[sh:node :Date]
			[sh:node :DateTime]
		);
	];
	sh:property [
		sh:path schema:validIn;
		sh:node :AdministrativeArea;
	];
	sh:property [
		sh:path schema:validUntil;
		sh:node :Date;
	].