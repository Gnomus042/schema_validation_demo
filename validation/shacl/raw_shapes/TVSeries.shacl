:TVSeries a sh:NodeShape;
	sh:targetClass schema:TVSeries;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:actor;
		sh:node :Person;
	];
	sh:property [
		sh:path schema:actors;
		sh:node :Person;
	];
	sh:property [
		sh:path schema:containsSeason;
		sh:node :CreativeWorkSeason;
	];
	sh:property [
		sh:path schema:countryOfOrigin;
		sh:node :Country;
	];
	sh:property [
		sh:path schema:director;
		sh:node :Person;
	];
	sh:property [
		sh:path schema:directors;
		sh:node :Person;
	];
	sh:property [
		sh:path schema:episode;
		sh:node :Episode;
	];
	sh:property [
		sh:path schema:episodes;
		sh:node :Episode;
	];
	sh:property [
		sh:path schema:musicBy;
		sh:or (
			[sh:node :Person]
			[sh:node :MusicGroup]
		);
	];
	sh:property [
		sh:path schema:numberOfEpisodes;
		sh:node :Integer;
	];
	sh:property [
		sh:path schema:numberOfSeasons;
		sh:node :Integer;
	];
	sh:property [
		sh:path schema:productionCompany;
		sh:node :Organization;
	];
	sh:property [
		sh:path schema:season;
		sh:or (
			[sh:node :CreativeWorkSeason]
			[sh:node :URL]
		);
	];
	sh:property [
		sh:path schema:seasons;
		sh:node :CreativeWorkSeason;
	];
	sh:property [
		sh:path schema:trailer;
		sh:node :VideoObject;
	].