:MusicRecording a sh:NodeShape;
	sh:targetClass schema:MusicRecording;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:byArtist;
		sh:or (
			[sh:node :MusicGroup]
			[sh:node :Person]
		);
	];
	sh:property [
		sh:path schema:duration;
		sh:node :Duration;
	];
	sh:property [
		sh:path schema:inAlbum;
		sh:node :MusicAlbum;
	];
	sh:property [
		sh:path schema:inPlaylist;
		sh:node :MusicPlaylist;
	];
	sh:property [
		sh:path schema:isrcCode;
		sh:node :Text;
	];
	sh:property [
		sh:path schema:recordingOf;
		sh:node :MusicComposition;
	].