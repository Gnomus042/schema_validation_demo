:Brand a sh:NodeShape;
	sh:targetClass schema:Brand;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:aggregateRating;
		sh:node :AggregateRating;
	];
	sh:property [
		sh:path schema:logo;
		sh:or (
			[sh:node :ImageObject]
			[sh:node :URL]
		);
	];
	sh:property [
		sh:path schema:review;
		sh:node :Review;
	];
	sh:property [
		sh:path schema:slogan;
		sh:node :Text;
	].