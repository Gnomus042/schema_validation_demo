:TaxiReservation a sh:NodeShape;
	sh:targetClass schema:TaxiReservation;
	sh:nodeKind sh:BlankNodeOrIRI;
	sh:property [
		sh:path schema:partySize;
		sh:or (
			[sh:node :QuantitativeValue]
			[sh:node :Integer]
		);
	];
	sh:property [
		sh:path schema:pickupLocation;
		sh:node :Place;
	];
	sh:property [
		sh:path schema:pickupTime;
		sh:node :DateTime;
	].